<h1>Acebook</h1>
<p>Welcome to Acebook!</p>
<%= form_with scope: :user, local: true do |form| %>
  <p>
    <%= form.label :username %><br>
    <%= form.text_field :username %>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.text_field :pass %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>